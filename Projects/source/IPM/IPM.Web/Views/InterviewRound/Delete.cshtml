@using PagedList.Mvc
@model PagedList.IPagedList<IPM.Model.Models.InterviewRound>


<div class="modal-content">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">@IPM.Web.MultiLanguage.Resource.DeleteRound</h4>
    </div>
    <div class="modal-body">

        @using (Ajax.BeginForm("Delete", "InterviewRound", FormMethod.Post, new AjaxOptions { InsertionMode = InsertionMode.Replace, HttpMethod = "POST", UpdateTargetId = "interviewRound" }))

        {
            <div class="alert alert-warning">
                <strong>Warning!</strong>Are you sure delete this item ?
            </div>
            <div class="box-footer">
            </div>
            <div class="modal-footer">
                <button type="submit" class="btn btn-primary btn-flat pull-right"><span class="glyphicon"></span>@IPM.Web.MultiLanguage.Resource.Delete</button>
                <button type="button" class="btn btn-default pull-left" data-dismiss="modal"><span class="glyphicon "></span>@IPM.Web.MultiLanguage.Resource.Cancel</button>
            </div>
        }
    </div>
    <!--/.col (right) -->


</div>

